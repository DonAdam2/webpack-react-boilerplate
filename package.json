    "jest-watch-typeahead": "^2.2.2",
    "mini-css-extract-plugin": "2.7.6",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "plop": "^4.0.0",
    "plop-action-eslint": "^0.4.0",
    "postcss": "^8.4.32",
