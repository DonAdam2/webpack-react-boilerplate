    "serve": "^14.0.1",
    "source-map-loader": "^4.0.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.5",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "axios": "^0.27.2",
    "crypto-js": "^4.1.1",
    "lodash": "^4.17.21",
    "react-error-boundary": "^3.1.4",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0"
  }
}
