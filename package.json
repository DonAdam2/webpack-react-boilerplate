    "sass-loader": "^13.3.2",
    "serve": "^14.2.1",
    "source-map-loader": "^4.0.1",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "to-camel-case": "^1.0.0",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.10.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "crypto-js": "^4.2.0",
