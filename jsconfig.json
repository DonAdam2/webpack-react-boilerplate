{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "esnext",
    "paths": {
      "@/assets/*": ["./src/assets/*"],
      "@/components/*": ["./src/components/*"],
      "@/constants/*": ["./src/constants/*"],
      "@/hooks/*": ["./src/hooks/*"],
      "@/jest/*": ["./jest/*"],
      "@/managers/*": ["./src/managers/*"],
      "@/pages/*": ["./src/pages/*"],
      "@/public/*": ["./public/*"],
      "@/routing/*": ["./src/routing/*"],
      "@/scss/*": ["./src/scss/*"],
      "@/services/*": ["./src/services/*"],
      "@/store/*": ["./src/store/*"]
    },
    "target": "es5"
  },
  "exclude": ["node_modules"]
}
