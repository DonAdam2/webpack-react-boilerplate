{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "baseUrl": ".",
    "paths": {
      "@/jest/*": ["./jest/*"],
      "@/assets/*": ["./src/assets/*"],
      "@/constants/*": ["./src/constants/*"],
      "@/managers/*": ["./src/managers/*"],
      "@/routing/*": ["./src/routing/*"],
      "@/store/*": ["./src/store/*"],
      "@/services/*": ["./src/services/*"],
      "@/hooks/*": ["./src/hooks/*"],
      "@/pages/*": ["./src/pages/*"],
      "@/components/*": ["./src/components/*"],
      "@/scss/*": ["./src/scss/*"],
      "@/public/*": ["./public/*"]
    }
  },
  "exclude": ["node_modules"]
}
